{
    "name": "codex-editor-extension",
    "displayName": "Codex Translation Editor",
    "description": "Support for `.codex` notebooks and translation source files. Includes Codex Language Server for translation drafting and checking diagnostics and features.",
    "publisher": "project-accelerate",
    "homepage": "https://codex-editor.gitbook.io/",
    "repository": {
        "type": "git",
        "url": "https://github.com/genesis-ai-dev/codex-editor"
    },
    "license": "MIT",
    "version": "0.6.0",
    "engines": {
        "node": ">=16.17.1",
        "vscode": "^1.78.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCustomEditor:sourceUploadProvider-scheme",
        "onCustomEditor:startupFlowProvider-scheme",
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "permissions": [
        "readFile"
    ],
    "contributes": {
        "keybindings": [],
        "views": {
            "codex-main-menu": [
                {
                    "type": "webview",
                    "id": "codex-editor.mainMenu",
                    "name": "Main Menu",
                    "icon": "$(list-tree)",
                    "contextualTitle": "Codex Main Menu"
                }
            ],
            "search-passages-view": [
                {
                    "type": "webview",
                    "id": "search-passages-sidebar",
                    "name": "Search Passages",
                    "icon": "$(eye)",
                    "contextualTitle": "Search Passages"
                }
            ],
            "codex-files-view": [
                {
                    "type": "webview",
                    "id": "codex-editor.navigation",
                    "name": "Navigation",
                    "icon": "$(compass)",
                    "contextualTitle": "Navigation"
                }
            ],
            "comments-sidebar-view": [
                {
                    "type": "webview",
                    "id": "comments-sidebar",
                    "name": "Comments",
                    "icon": "$(note)",
                    "contextualTitle": "Comments"
                }
            ],
            "project-manager": [
                {
                    "type": "webview",
                    "id": "project-manager-sidebar",
                    "name": "Project Manager",
                    "icon": "$(tools)",
                    "contextualTitle": "Project Manager"
                }
            ],
            "explorer": [
                {
                    "id": "codex-project-manager.welcomeView",
                    "name": "Codex Project Manager",
                    "when": "workbenchState == empty"
                },
                {
                    "type": "webview",
                    "id": "codex-editor.editAnalysis",
                    "name": "AI Metrics"
                }
            ]
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "codex-main-menu",
                    "title": "Codex Main Menu",
                    "icon": "$(list-tree)"
                },
                {
                    "id": "codex-files-view",
                    "title": "Navigation",
                    "icon": "$(compass)"
                },
                {
                    "id": "project-manager",
                    "title": "Project Manager",
                    "icon": "$(tools)"
                },
                {
                    "id": "search-passages-view",
                    "title": "Search Passages",
                    "icon": "$(eye)"
                },
                {
                    "id": "comments-sidebar-view",
                    "title": "Comments",
                    "icon": "$(note)"
                }
            ]
        },
        "menus": {
            "view/title": [
                {
                    "command": "dictionaryTable.showDictionaryTable",
                    "when": "view == dictionaryTable",
                    "group": "navigation"
                }
            ],
            "view/item/context": [
                {
                    "command": "dictionaryTable.showDictionaryTable",
                    "when": "view == dictionaryTable",
                    "group": "navigation"
                }
            ],
            "editor/title": [
                {
                    "command": "codex-editor-extension.scm.stageAndCommitAll",
                    "group": "navigation"
                }
            ],
            "commandPalette": [
                {
                    "command": "codex-editor-extension.getFileStats",
                    "title": "Codex Extension: Show File Statistics"
                }
            ]
        },
        "commands": [
            {
                "command": "codex-project-manager.openNewSourceUpload",
                "title": "Open New Source Uploader",
                "category": "Codex"
            },
            {
                "command": "codex-editor-extension.JOSH_COMMAND",
                "title": "JOSH_COMMAND"
            },
            {
                "command": "codex-editor-extension.forceReindex",
                "title": "Force Reindex",
                "icon": "$(refresh)",
                "category": "Codex Extension"
            },
            {
                "title": "Start a new Project",
                "shortTitle": "Start a new Project",
                "command": "codex-editor-extension.initializeNewProject"
            },
            {
                "command": "codex-editor-extension.downloadSourceBible",
                "title": "Download Source Bible"
            },
            {
                "command": "codex-notebook-extension.openFile",
                "title": "Open File"
            },
            {
                "command": "codex-editor-extension.indexVrefs",
                "title": "indexVrefs"
            },
            {
                "command": "codex-editor-extension.searchIndex",
                "title": "searchIndex"
            },
            {
                "command": "codex-editor-extension.showReferences",
                "title": "Show Scripture References"
            },
            {
                "command": "codex-editor-extension.showSource",
                "title": "Show Scripture Source"
            },
            {
                "command": "codex-editor-extension.createCodexNotebook",
                "title": "Create Codex Notebook"
            },
            {
                "command": "codex-editor-extension.updateProjectNotebooksToUseCellsForVerseContent",
                "title": "Update Project Notebooks to Use Cells for Verse Content"
            },
            {
                "command": "codex-notebook-extension.createCodexProject",
                "title": "Create Codex Project"
            },
            {
                "command": "codex-editor-extension.analyzeEdits",
                "title": "Analyze Edits"
            },
            {
                "command": "dictionaryTable.showDictionaryTable",
                "title": "Dictionary Table: Show"
            },
            {
                "command": "translationNotes.openTnEditor",
                "title": "Open Translation Notes"
            },
            {
                "command": "codex-editor-extension.scm.stageAndCommitAll",
                "title": "Stage & Commit All"
            },
            {
                "command": "codex-editor-extension.scm.addRemote",
                "title": "Add Remote"
            },
            {
                "command": "codex-editor-extension.scm.sync",
                "title": "Sync"
            },
            {
                "command": "codex-editor-extension.getTranslationPairFromProject",
                "title": "Get Translation Pair from Project",
                "category": "Codex Extension"
            },
            {
                "command": "codex-editor-extension.showIndexOptions",
                "title": "Show Index Options",
                "category": "Codex Extension"
            },
            {
                "command": "codex-editor-extension.setEditorFontToTargetLanguage",
                "title": "Set Editor Font to Target Language",
                "category": "Codex Editor"
            },
            {
                "command": "codex-editor-extension.exportCodexContent",
                "title": "Export Codex Translation as Plain Text",
                "category": "Codex Editor"
            },
            {
                "command": "codex-project-manager.initializeNewProject",
                "title": "Initialize New Project",
                "category": "Codex Project Management"
            },
            {
                "command": "codex-project-manager.nameProject",
                "title": "Name Project",
                "category": "Codex Project Management"
            },
            {
                "command": "codex-project-manager.openProjectSettings",
                "title": "Open Project Settings",
                "category": "Codex Project Management"
            },
            {
                "command": "codex-project-manager.startWalkthrough",
                "title": "Start Walkthrough",
                "category": "Codex Project Management"
            },
            {
                "command": "codex-project-manager.startTranslating",
                "title": "Start Translating",
                "category": "Codex Project Management"
            },
            {
                "command": "codex-project-manager.downloadTargetBible",
                "title": "Download Target Bible",
                "category": "Codex Project Management"
            },
            {
                "command": "codex-project-manager.editAbbreviation",
                "title": "Edit Abbreviation",
                "category": "Codex Project Management"
            },
            {
                "command": "codex-project-manager.setValidationCount",
                "title": "Set Validation Count",
                "category": "Codex Project Management"
            },
            {
                "command": "codex-project-manager.reinstallExtensions",
                "title": "Reinstall Extensions",
                "category": "Codex Project Management"
            },
            {
                "command": "codex-project-manager.importLocalUsfmSourceBible",
                "title": "Import Local USFM Source Bible",
                "category": "Codex Project Management"
            },
            {
                "command": "codex-project-manager.updateGitignore",
                "title": "Update .gitignore",
                "category": "Codex Project Management"
            },
            {
                "command": "codex-project-manager.openSourceUpload",
                "title": "Open Source Upload"
            },
            {
                "command": "codex-project-manager.openStartupFlow",
                "title": "Open Startup Flow",
                "category": "Codex Project Manager"
            },
            {
                "command": "codex-project-manager.triggerMigration",
                "title": "Trigger Project Migration",
                "category": "Codex Project Manager"
            },
            {
                "command": "codex-editor-extension.getTranslationPairsFromSourceCellQuery",
                "title": "Search Source Cells"
            },
            {
                "command": "codex-editor-extension.getSourceCellByCellIdFromAllSourceCells",
                "title": "Get Source Cell by Cell ID"
            },
            {
                "command": "codex-editor-extension.getTranslationPairFromProject",
                "title": "Get Translation Pair from Project"
            },
            {
                "command": "codex-editor-extension.getTranslationPairsFromSourceCellQuery",
                "title": "Get Translation Pairs from Source Cell Query"
            },
            {
                "command": "codex-editor-extension.getTargetCellByCellId",
                "title": "Get Target Cell by Cell ID"
            },
            {
                "command": "extension.lookupWord",
                "title": "Look Up Word",
                "category": "Dictionary"
            },
            {
                "command": "extension.importWiktionaryJSONL",
                "title": "Import Wiktionary"
            },
            {
                "command": "nextGenCodexTreeView.openFile",
                "title": "Open Codex File",
                "icon": "$(book)"
            },
            {
                "command": "frontier.showWordsView",
                "title": "Frontier: View All Words",
                "category": "Frontier"
            },
            {
                "command": "codex-project-manager.openAISettings",
                "title": "Open AI Settings",
                "category": "Codex Project Management"
            },
            {
                "command": "codex-project-manager.openLicenseSettings",
                "title": "Open License Settings",
                "category": "Codex Project Management"
            },
            {
                "command": "codex-project-manager.openExportView",
                "title": "Open Export View"
            },
            {
                "command": "codex-editor-extension.getFileStats",
                "title": "Codex Extension: Show File Statistics"
            },
            {
                "command": "codex-editor-extension.getFileInfo",
                "title": "Codex Extension: Show Detailed File Information"
            },
            {
                "command": "codex-editor-extension.forceIntegrityValidation",
                "title": "Codex: Run Integrity Validation",
                "category": "Codex Validation"
            },
            {
                "command": "codex-editor-extension.checkValidationStatus",
                "title": "Codex: Check Validation Service Status",
                "category": "Codex Validation"
            }
        ],
        "configuration": [
            {
                "title": "Codex Project Manager",
                "properties": {
                    "codex-project-manager.projectName": {
                        "type": "string",
                        "default": "",
                        "description": "Name of the project"
                    },
                    "codex-project-manager.validationCount": {
                        "type": "integer",
                        "default": 1,
                        "description": "Number of required validations for each translation",
                        "minimum": 1,
                        "maximum": 15
                    },
                    "codex-project-manager.userName": {
                        "type": "string",
                        "default": "",
                        "description": "Name of the user"
                    },
                    "codex-project-manager.userEmail": {
                        "type": "string",
                        "default": "",
                        "description": "Email of the user e.g. user@example.com"
                    },
                    "codex-project-manager.abbreviation": {
                        "type": "string",
                        "default": "",
                        "description": "Project abbreviation"
                    },
                    "codex-project-manager.sourceLanguage": {
                        "type": "object",
                        "default": {},
                        "description": "Source language for the project",
                        "properties": {
                            "tag": {
                                "type": "string"
                            },
                            "refName": {
                                "type": "string"
                            },
                            "projectStatus": {
                                "type": "string"
                            }
                        }
                    },
                    "codex-project-manager.targetLanguage": {
                        "type": "object",
                        "default": {},
                        "description": "Target language for the project",
                        "properties": {
                            "tag": {
                                "type": "string"
                            },
                            "refName": {
                                "type": "string"
                            },
                            "projectStatus": {
                                "type": "string"
                            }
                        }
                    },
                    "codex-project-manager.primarySourceText": {
                        "type": "string",
                        "default": "",
                        "description": "Path to the primary source text for the project"
                    },
                    "codex-project-manager.knownProjects": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        },
                        "default": [],
                        "description": "List of known Codex project paths"
                    },
                    "codex-project-manager.watchedFolders": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        },
                        "default": [],
                        "description": "List of folders to watch for Codex projects"
                    },
                    "codex-project-manager.projectHistory": {
                        "type": "object",
                        "default": {},
                        "description": "History of project access times",
                        "additionalProperties": {
                            "type": "string"
                        }
                    },
                    "codex-project-manager.spellcheckIsEnabled": {
                        "type": "boolean",
                        "default": true,
                        "description": "Enable or disable spellcheck for the project"
                    },
                    "codex-project-manager.autoSyncEnabled": {
                        "type": "boolean",
                        "default": true,
                        "description": "Enable automatic synchronization of changes to remote repository"
                    },
                    "codex-project-manager.syncDelayMinutes": {
                        "type": "number",
                        "enum": [
                            1,
                            5,
                            15,
                            30,
                            60
                        ],
                        "default": 5,
                        "description": "Delay in minutes before triggering automatic sync after changes"
                    },
                    "codex-project-manager.migration.skipSyncVerification": {
                        "type": "boolean",
                        "default": false,
                        "description": "Skip remote synchronization verification during project migration. Warning: This may result in data loss if local changes haven't been pushed to remote."
                    }
                }
            },
            {
                "title": "Codex Editor: Cloud Sync",
                "properties": {
                    "codex-editor-extension.scm.remoteUrl": {
                        "type": "string",
                        "default": "",
                        "description": "The remote URL to cloud sync to."
                    },
                    "codex-editor-extension.scm.autoCommit": {
                        "type": "boolean",
                        "default": true,
                        "description": "Enable or disable auto-commit feature."
                    },
                    "codex-editor-extension.scm.autoSyncInterval": {
                        "type": "string",
                        "enum": [
                            "Do not auto sync",
                            "Every 1 minute",
                            "Every 5 minutes",
                            "Every 15 minutes",
                            "Every hour",
                            "Every day"
                        ],
                        "default": "Do not auto sync",
                        "description": "Automatically Sync to Cloud every..."
                    }
                }
            },
            {
                "title": "Codex Extension Server",
                "properties": {
                    "codex-editor-extension-server.enable": {
                        "type": "boolean",
                        "default": true,
                        "description": "Enable or disable the Codex Extension feature."
                    },
                    "codex-editor-extension-server.primarySourceText": {
                        "type": "string",
                        "default": "",
                        "description": "The source text to use for the Codex Extension feature."
                    }
                }
            },
            {
                "title": "Codex Extension",
                "properties": {
                    "codex-editor-extension.api_key": {
                        "title": "API Key",
                        "type": "string",
                        "default": "",
                        "description": "Your own OpenAI API key. Ignorable for local LLM. NOTE: Do not set this in workspace settings. This may compromise your key if .vscode/settings.json is saved to the cloud."
                    },
                    "codex-editor-extension.max_tokens": {
                        "title": "Max Tokens",
                        "type": "number",
                        "default": 2048,
                        "description": "The maximum number of output tokens for the completion API."
                    },
                    "codex-editor-extension.temperature": {
                        "title": "Temperature",
                        "type": "number",
                        "default": 0.8,
                        "description": "The randomness level of output generation. A higher value leads to more random outputs."
                    },
                    "codex-editor-extension.main_chat_language": {
                        "title": "Main Chat Language",
                        "type": "string",
                        "default": "English",
                        "enum": [
                            "English",
                            "தமிழ் (Tamil)",
                            "తెలుగు (Telugu)",
                            "ಕನ್ನಡ (Kannada)",
                            "हिन्दी (Hindi)",
                            "ગુજરાતી (Gujarati)",
                            "Español (Spanish)",
                            "Français (French)",
                            "Deutsch (German)",
                            "Italiano (Italian)",
                            "Nederlands (Dutch)",
                            "Português (Portuguese)",
                            "Русский (Russian)",
                            "中文 (Chinese)",
                            "日本語 (Japanese)",
                            "한국어 (Korean)",
                            "العربية (Arabic)",
                            "Türkçe (Turkish)",
                            "Tiếng Việt (Vietnamese)",
                            "ไทย (Thai)",
                            "Bahasa Indonesia (Indonesian)",
                            "Bahasa Melayu (Malay)",
                            "Filipino (Filipino)",
                            "বাংলা (Bengali)",
                            "ਪੰਜਾਬੀ (Punjabi)",
                            "मराठी (Marathi)",
                            "ଓଡ଼ିଆ (Odia)",
                            "Kiswahili (Swahili)",
                            "اردو (Urdu)",
                            "فارسی (Persian)",
                            "Hausa",
                            "አማርኛ (Amharic)",
                            "ꦧꦱꦗꦮ (Javanese)",
                            "မြန်မာဘာသာ (Burmese)",
                            "Svenska (Swedish)",
                            "Norsk (Norwegian)",
                            "Suomi (Finnish)",
                            "Dansk (Danish)",
                            "עברית (Hebrew)",
                            "Українська (Ukrainian)",
                            "Polski (Polish)",
                            "Română (Romanian)",
                            "Čeština (Czech)",
                            "Magyar (Hungarian)",
                            "Ελληνικά (Greek)",
                            "Српски (Serbian)",
                            "Hrvatski (Croatian)",
                            "Български (Bulgarian)",
                            "Slovenčina (Slovak)",
                            "മലയാളം (Malayalam)",
                            "සිංහල (Sinhala)",
                            "ភាសាខ្មែរ (Khmer)",
                            "ພາສາລາວ (Lao)"
                        ],
                        "description": "The main language used for the chat interface."
                    },
                    "codex-editor-extension.chatSystemMessage": {
                        "title": "Rules for AI (System Message)",
                        "type": "string",
                        "default": "This is a chat between a helpful Bible translation assistant and a Bible translator. The assistant will provide helpful answers and suggestions to the translator, often relying on the translator's current project and reference resources. The translator will ask questions and provide context to the assistant. The translator's aim is to be consistent and faithful in a fairly literalistic rendering of the source text.",
                        "description": "The system message controlling the behavior of the chat system and inline completion. You can customize this message to fit your needs, or ask the copilot to translate it for you into your preferred interface language.",
                        "editPresentation": "multilineText"
                    },
                    "codex-editor-extension.sourceTextSelectionMode": {
                        "type": "string",
                        "default": "auto",
                        "description": "The method of selecting the source text. If set to manual, use 'ctrl + s, ctrl + b' to change the source text.",
                        "enum": [
                            "auto",
                            "manual"
                        ]
                    },
                    "codex-editor-extension.numberOfFewShotExamples": {
                        "type": "number",
                        "default": 30,
                        "description": "The number of sample translation pairs to show the Copilot."
                    },
                    "codex-editor-extension.contextSize": {
                        "type": "string",
                        "default": "medium",
                        "description": "The size of the context to be used for completion. Small is 5 cells, medium is 10 cells, large is 50 cells.",
                        "enum": [
                            "small",
                            "medium",
                            "large"
                        ]
                    },
                    "codex-editor-extension.additionalResourcesDirectory": {
                        "type": "string",
                        "default": "",
                        "description": "The directory path containing additional resources for completion context. Will read all files in directory and put them in the context. No resources if path is invalid."
                    },
                    "codex-editor-extension.llmEndpoint": {
                        "type": "string",
                        "default": "https://api.openai.com/v1",
                        "description": "The endpoint for the completion API. Currently only supports OpenAI API, or openai-compatible local endpoints such as LM Studio, etc."
                    },
                    "codex-editor-extension.wsEndpoint": {
                        "type": "string",
                        "default": "wss://api.openai.com/v1/chat/completions",
                        "description": "The endpoint for the realtime chat API. Currently only supports OpenAI API."
                    },
                    "codex-editor-extension.model": {
                        "title": "LLM Model",
                        "type": "string",
                        "enum": [
                            "custom",
                            "gpt-4o",
                            "gpt-4o-mini",
                            "davinci"
                        ],
                        "default": "gpt-4o",
                        "description": "Model name selected for inference. Ignorable for local LLM. If you are using a custom LLM, set to 'custom' and update the customModel setting below."
                    },
                    "codex-editor-extension.customModel": {
                        "title": "Custom LLM Model",
                        "type": "string",
                        "default": "",
                        "description": "Custom LLM model to use for inference. Ignorable for OpenAI API."
                    },
                    "codex-editor-extension.sourceBookWhitelist": {
                        "title": "Source Book Whitelist",
                        "type": "string",
                        "default": "",
                        "description": "Limits context building to specified books. Leave empty to include all. We recommend to leave empty for best results."
                    },
                    "codex-editor-extension.debugMode": {
                        "title": "Debugging Mode",
                        "type": "boolean",
                        "default": false,
                        "description": "If activated, inline completion LLM prompts will be saved to the `copilot-messages.log` file in the project."
                    },
                    "codex-editor-extension.wordFrequencyThreshold": {
                        "title": "Word Frequency Threshold",
                        "type": "number",
                        "default": 50,
                        "description": "The minimum frequency of a word to be automatically added to the dictionary. Changing this value will trigger updates to the dictionary."
                    },
                    "codex-editor-extension.enableDoctrineGrading": {
                        "title": "Enable Doctrine Grading",
                        "type": "boolean",
                        "default": false,
                        "description": "Enable or disable doctrine grading."
                    },
                    "codex-editor-extension.enableChatReflection": {
                        "title": "Enable Chat Reflection",
                        "type": "boolean",
                        "default": false,
                        "description": "After giving an initial response in the chat, the Copilot will reflect on the response and replace it with a new one after a bit."
                    },
                    "codex-editor-extension.chatReflectionConcern": {
                        "title": "Chat Reflection Concern",
                        "type": "string",
                        "default": "",
                        "description": "This is an optional field to specify what the reflection should focus on in the chat response.  Use this to set a specific topic to focus on or perspective to keep in mind."
                    },
                    "codex-editor-extension.textDirection": {
                        "type": "string",
                        "enum": [
                            "ltr",
                            "rtl"
                        ],
                        "default": "ltr",
                        "description": "Text direction for the editor (left-to-right or right-to-left)"
                    },
                    "codex-editor-extension.cellsPerPage": {
                        "type": "number",
                        "default": 50,
                        "minimum": 5,
                        "maximum": 200,
                        "description": "Number of cells to display per page when a chapter has many cells. Helps with performance for large chapters."
                    }
                }
            }
        ],
        "customEditors": [
            {
                "viewType": "sourceUploadProvider",
                "displayName": "Source Upload Editor",
                "selector": [
                    {
                        "filenamePattern": "*.sourceUploadProvider",
                        "scheme": "sourceUploadProvider-scheme"
                    }
                ],
                "priority": "default"
            },
            {
                "viewType": "newSourceUploaderProvider",
                "displayName": "New Source Uploader",
                "selector": [
                    {
                        "filenamePattern": "*.newSourceUploaderProvider",
                        "scheme": "newSourceUploaderProvider-scheme"
                    }
                ],
                "priority": "default"
            },
            {
                "viewType": "startupFlowProvider",
                "displayName": "Startup Flow Editor",
                "selector": [
                    {
                        "filenamePattern": "*.startupFlowProvider",
                        "scheme": "startupFlowProvider-scheme"
                    }
                ],
                "priority": "default"
            },
            {
