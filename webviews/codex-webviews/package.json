        "date-fns": "^4.1.0",
        "diff": "^5.2.0",
        "dompurify": "^3.0.9",
        "faker": "^5.5.3",
        "framer-motion": "^11.0.15",
        "immutability-helper": "^3.1.1",
        "lodash.debounce": "^4.0.8",
        "marked": "^12.0.0",
        "pnpm": "^10.6.2",
        "process": "^0.11.10",
        "quill": "^2.0.3",
        "quill-delta": "^5.1.0",
        "quilljs-markdown": "^1.2.0",
        "react": "^18.2.0",
        "react-contenteditable": "^3.3.5",
