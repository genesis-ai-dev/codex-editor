{
    "name": "my-vite-project",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build:ChatView": "cross-env APP_NAME=ChatView vite build",
        "build:StartupFlow": "cross-env APP_NAME=StartupFlow vite build",
        "build:ParallelView": "cross-env APP_NAME=ParallelView vite build",
        "build:CodexCellEditor": "cross-env APP_NAME=CodexCellEditor vite build",
        "build:SemanticView": "cross-env APP_NAME=SemanticView vite build",
        "build:CommentsView": "cross-env APP_NAME=CommentsView vite build",
        "build:CodexFilesView": "cross-env APP_NAME=CodexFilesView vite build",
        "build:EditableReactTable": "cross-env APP_NAME=EditableReactTable vite build",
        "build:ProjectManagerView": "cross-env APP_NAME=ProjectManagerView vite build",
        "build:SourceUpload": "cross-env APP_NAME=SourceUpload vite build",
        "build:all": "pnpm run build:ProjectManagerView && pnpm run build:ParallelView && pnpm run build:SemanticView && pnpm run build:ChatView && pnpm run build:CommentsView && pnpm run build:CodexFilesView",
        "type-check": "tsc --noEmit",
        "watch:all": "nodemon --watch src --ext ts,tsx,css --exec pnpm run build:all",
        "watch:ChatView": "nodemon --watch src --ext ts,tsx,css --exec pnpm run build:ChatView",
        "watch:CodexCellEditor": "nodemon --watch src --ext ts,tsx,css --exec pnpm run build:CodexCellEditor",
        "watch:SemanticView": "nodemon --watch src --ext ts,tsx,css --exec pnpm run build:SemanticView",
        "watch:SourceUpload": "nodemon --watch src --ext ts,tsx,css --exec pnpm run build:SourceUpload",
        "watch:CommentsView": "nodemon --watch src --ext ts,tsx,css --exec pnpm run build:CommentsView",
        "watch:ParallelView": "nodemon --watch src --ext ts,tsx,css --exec pnpm run build:ParallelView",
        "watch:StartupFlow": "nodemon --watch src --ext ts,tsx,css --exec pnpm run build:StartupFlow",
        "watch:EditableReactTable": "nodemon --watch src --ext ts,tsx,css --exec pnpm run build:EditableReactTable",
        "watch:ProjectManagerView": "nodemon --watch src --ext ts,tsx,css --exec pnpm run build:ProjectManagerView",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview"
    },
    "dependencies": {
        "@popperjs/core": "^2.0.0",
