        "react-markdown": "^9.0.1",
        "react-papaparse": "^4.4.0",
        "react-player": "^2.16.0",
        "react-popper": "^2.2.5",
        "react-table": "^7.8.0",
        "rehype-parse": "^9.0.0",
        "rehype-raw": "^7.0.0",
        "rehype-remark": "^10.0.0",
        "remark": "^15.0.1",
        "remark-html": "^16.0.1",
        "remark-stringify": "^11.0.0",
        "stream-browserify": "^3.0.0",
        "util": "^0.12.5",
        "uuid": "^9.0.1",
        "webvtt-parser": "^2.2.0",
        "xstate": "^5.19.0"
    },
    "devDependencies": {
        "@babel/plugin-transform-private-property-in-object": "^7.16.7",
